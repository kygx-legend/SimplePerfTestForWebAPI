{
    "html5": true,
    "html": {
        "head": {
            "css": [
                { "href": "css/perf_test.css" }
            ],
            "script": [
                { "src": "js/perf_test.js" }
            ],
            "title": "Performance Test Index",
            "meta": {
                "charset": "utf-8"
            }
        },
        "body": {
            "div": {
                "class": "main",
                "div": [{
                    "class": "title",
                    "h2": "Performance Test Index"
                },{
                    "class": "description",
                    "p": [{
                        "class": "case",
                        "text": "space"
                    },{
                        "id": "content",
                        "class": "case",
                        "text": null
                    }],
                    "div": [{
                        "input": {
                            "type": "text",
                            "id": "scale",
                            "value": "1"
                        },
                        "text": "case = unit * scale"
                    },{
                        "input": {
                            "type": "text",
                            "id": "number",
                            "value": "10"
                        },
                        "text": "cases number:"
                    },{
                        "input": {
                            "type": "text",
                            "id": "method",
                            "value": "linear"
                        },
                        "text": "test methods: "
                    },{
                        "input": {
                            "type": "text",
                            "id": "variable",
                            "value": "2"
                        },
                        "text": "if linear: v=k, else: v=p(integer)"
                    },{
                        "text": "Model: y = k*x + x / pow(p*x, 2) for n cases"
                    }],
                    "button": {
                        "id": "testBtn",
                        "text": "Test"
                    },
                    "h5": "Test Result Diagram:"
                },{
                    "div": {
                        "class": "resutl",
                        "div": [{
                            "id": "result1",
                            "text": ""
                        },{
                            "id": "result2",
                            "text": ""
                        }]
                    }
                }]
            },
            "script": [
                { "src": "js/flotr2.min.js" }
            ]
        }
    }
}
